package com.capgemini.springboot.microserviceconcurrencyconversion.entity;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.cloud.openfeign.EnableFeignClients;

@SpringBootApplication
@EnableFeignClients(" com.capgemini.springboot.microserviceconcurrencyconversion.entity;")
@EnableDiscoveryClient
public class SpringBootMicroserviceCurrencyConversionApplication {
   
	 public static void main(String[] args) {
		    SpringApplication.run(SpringBootMicroserviceCurrencyConversionApplication.class, args);
		  }

}
